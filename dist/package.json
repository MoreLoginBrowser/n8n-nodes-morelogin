{
    "name": "n8n-nodes-morelogin",
    "version": "0.1.0",
    "description": "Morelogin node for n8n",
    "license": "MIT",
    "homepage": "",
    "keywords": ["n8n", "n8n-community-node-package", "n8n-community-node-package", "morelogin"],
    "author": {
        "name": "morelogin",
        "email": "developer@morelogin.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/MoreLoginBrowser/n8n-nodes-morelogin.git"
    },
    "scripts": {
        "build": "n8n-node build",
        "build:watch": "tsc --watch",
        "dev": "n8n-node dev",
        "lint": "n8n-node lint",
        "lint:fix": "n8n-node lint --fix",
        "release": "n8n-node release",
        "prepublishOnly": "n8n-node prerelease"
    },
    "files": [
        "dist",
        "nodes",
        "credentials",
        "icons"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "strict": true,
        "credentials": [
            "credentials/moreLoginOAuth2Api.credentials.ts"
        ],
        "nodes": [
            "nodes/morelogin/morelogin.node.ts"
        ]
    },
    "devDependencies": {
        "@n8n/node-cli": "*",
        "@types/node": "^25.0.3",
        "eslint": "9.32.0",
        "prettier": "3.6.2",
        "release-it": "^19.0.4",
        "typescript": "5.9.2"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    }
}
